from selenium import webdriver
from time import sleep

# url = "http://www.webscrapingfordatascience.com/complexjavascript/"
url = "https://character-sheets.appspot.com/mgr/edit.html?key=ahVzfmNoYXJhY3Rlci1zaGVldHMtbXByFwsSDUNoYXJhY3RlckRhdGEY7oLBqQEM"

driver = webdriver.Chrome()
driver.get(url)

sleep(3);

#ネーム取得
baseplayer = driver.find_element_by_id("base.name")
print(baseplayer.get_attribute("value"))






#input("Press ENTER to Close the automated browser")
driver.quit()
